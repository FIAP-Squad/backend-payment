    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.6",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "module-alias": "^2.2.3",
    "prisma": "^5.14.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@types/mongodb": "^4.0.7",
    "cli-color": "^2.0.3",
    "express": "^4.18.2",
    "mongodb": "^6.8.0",
    "validator": "^13.11.0"
  },
  "engines": {
    "node": "v18.x"
  },
